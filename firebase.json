{
    "hosting": {
      "public": "dist/",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
       "rewrites": [ 
        {
        "source": "**",
        "destination": "/index.html"
        } ],
      "headers": [ {
        "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
        "headers" : [ {
          "key" : "Access-Control-Allow-Origin",
          "value" : "http://gmm-student.fc.utm.my/~rlbna"
        } ]
        }, {
        "source" : "**/*.@(jpg|jpeg|gif|png)",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        } ]
        }, {
        "source" : "404.html",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=300"
        } ]
      } ],
      "cleanUrls": true,
      "trailingSlash": false
    }
  }